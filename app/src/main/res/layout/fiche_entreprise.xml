<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BFBDBD">


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="376dp"
        android:layout_height="237dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.192"
        app:srcCompat="@drawable/carre_blancg" />

    <TextView
        android:id="@+id/nomFournisseur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

    <TextView
        android:id="@+id/adresseF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="76dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur" />

    <TextView
        android:id="@+id/site"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="76dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="76dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur" />

    <TextView
        android:id="@+id/description"
        android:layout_width="220dp"
        android:layout_height="45dp"
        android:layout_marginTop="124dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/nomFournisseur"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur" />

    <Button
        android:id="@+id/chooseButton"
        android:layout_width="99dp"
        android:layout_height="36dp"
        android:layout_marginTop="172dp"
        android:text="Choisir"
        app:layout_constraintEnd_toEndOf="@+id/nomFournisseur"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/echelle_prix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/nomFournisseur"
        app:layout_constraintTop_toBottomOf="@+id/nomFournisseur" />
</androidx.constraintlayout.widget.ConstraintLayout>