<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".client.ThirdFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="201dp"
            android:layout_height="82dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.07999998"
            app:srcCompat="@drawable/carre_blanc" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="361dp"
            android:layout_height="243dp"
            android:layout_marginTop="100dp"
            app:layout_constraintEnd_toEndOf="@id/imageView16"
            app:layout_constraintStart_toStartOf="@+id/imageView16"
            app:layout_constraintTop_toBottomOf="@+id/imageView16"
            app:srcCompat="@drawable/group_2" />

        <TextView
            android:id="@+id/ClientPrenom"
            android:layout_width="329dp"
            android:layout_height="18dp"
            android:layout_marginTop="72dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/ClientMail"
            app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintStart_toStartOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15"
            app:layout_constraintVertical_bias="0.17000002" />

        <TextView
            android:id="@+id/ClientMdp"
            android:layout_width="329dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="184dp"
            android:text="TextView"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15" />

        <TextView
            android:id="@+id/ClientNum"
            android:layout_width="329dp"
            android:layout_height="18dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="144dp"
            android:text="TextView"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15" />

        <TextView
            android:id="@+id/ClientMail"
            android:layout_width="329dp"
            android:layout_height="20dp"
            android:layout_marginTop="108dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15" />

        <TextView
            android:id="@+id/ClientNom"
            android:layout_width="329dp"
            android:layout_height="20dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/ClientPrenom"
            app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15"
            app:layout_constraintVertical_bias="0.69" />

        <Button
            android:id="@+id/deco_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#F9B25F"
            android:text="Deconnexion"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.87" />

        <TextView
            android:id="@+id/title_profile"
            android:layout_width="123dp"
            android:layout_height="35dp"
            android:text="Votre Profil"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="@+id/imageView16"
            app:layout_constraintEnd_toEndOf="@+id/imageView16"
            app:layout_constraintStart_toStartOf="@+id/imageView16"
            app:layout_constraintTop_toTopOf="@+id/imageView16" />

    </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>