<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BFBDBD">





    <ImageButton
        android:id="@+id/coiffeurButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="40dp"
        android:background="#BFBDBD"
        android:onClick="onClick"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_coiffeur"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="right"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="50dp"
        android:background="#BFBDBD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/coiffeurButton"
        app:layout_constraintTop_toBottomOf="@+id/coiffeurButton"
        app:srcCompat="@drawable/icon_jardinier"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="50dp"
        android:background="#BFBDBD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.922"
        app:layout_constraintStart_toEndOf="@+id/coiffeurButton"
        app:layout_constraintTop_toBottomOf="@+id/coiffeurButton"
        app:srcCompat="@drawable/icon_mecanicien"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="left"
        android:layout_marginTop="200dp"
        android:layout_marginStart="70dp"
        android:background="#BFBDBD"
        app:layout_constraintStart_toEndOf="@+id/coiffeurButton"
        app:layout_constraintTop_toTopOf="@+id/coiffeurButton"
        app:srcCompat="@drawable/icon_medecin"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="left"
        android:layout_marginTop="360dp"
        android:layout_marginStart="70dp"
        android:background="#BFBDBD"
        app:layout_constraintStart_toStartOf="@+id/coiffeurButton"
        app:layout_constraintTop_toBottomOf="@+id/coiffeurButton"
        app:srcCompat="@drawable/icon_restaurant"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="right"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="50dp"
        android:background="#BFBDBD"
        app:layout_constraintStart_toStartOf="@+id/coiffeurButton"
        app:layout_constraintTop_toBottomOf="@+id/coiffeurButton"
        app:srcCompat="@drawable/icon_travaux"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />
</FrameLayout>